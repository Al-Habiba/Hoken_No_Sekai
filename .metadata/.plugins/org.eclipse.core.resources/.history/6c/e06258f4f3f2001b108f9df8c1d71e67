		menuBar.add(cdeao);
		fermer.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				Page_Accueil window = new Page_Accueil();
				window.accueil.setVisible(true);
				frame.hide();
			}
		});
	}
}
